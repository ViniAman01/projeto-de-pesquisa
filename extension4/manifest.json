{
  "name": "SUAP-Extension",
  "description": "Pegar informações de horário",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage","activeTab","scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "SUAP",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "modal.html"
  ]
}
