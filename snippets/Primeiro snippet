//     }
// }
// console.log(string_horario_entrada)

string_horario_entrada = []
for(i in todos_horarios_pontos){
    if(char_eh_aula[i] == 'A'){
        string_horario_entrada.push(todos_horarios_pontos[i].textContent.split('E')[todos_horarios_pontos[i].textContent.split('E').length-1].split('S'))
    }
}
console.log(string_horario_entrada)

