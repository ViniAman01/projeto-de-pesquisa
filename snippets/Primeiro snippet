todos_horarios_pontos = document.querySelectorAll("td:nth-child(2)")
todos_horarios_aulas = document.querySelectorAll("td.ifms-code")
char_eh_aula = []
todos_horarios_aulas.forEach((element) => char_eh_aula.push(element.innerText[34]))
console.log(char_eh_aula)

// string_horario_entrada = []
